
@-webkit-keyframes slidefromleft {
from {
		display: none;
}
to {
		display: block;
}
}
@keyframes slidefromleft {
from {
		display: none;
}
to {
		display: block;
}
}
.leftgrandfather {
	padding-top: 44px;
	background: #fcfcfc;
	-webkit-animation: slidefromleft 3s;
	        animation: slidefromleft 3s;
	width: 174px;
	height: 100%;
	position: fixed;
	left: 0;
	top: 0;
	z-index: 1;
	-webkit-box-shadow: 1px 0px 5px #a6a6a6;
	        box-shadow: 1px 0px 5px #a6a6a6;
	-webkit-border-bottom-right-radius: 80px;
	        border-bottom-right-radius: 80px;
}
