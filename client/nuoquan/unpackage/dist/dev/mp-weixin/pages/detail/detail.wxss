
.articlecard {
	width:650rpx;
	-webkit-border-radius: 8px;
	        border-radius: 8px;
	margin: 11px auto 0;
	background-color: #FFFFFF;
}
.title {
	margin:16px 25px 0 25px;
	font-size: 15px;
	font:MicrosoftYaHei;
	font-weight: bold;
	line-height: 19px;
	margin: 16px 13px 0 15px;
	padding-top: 16px;
}
.briefarticlecontent {
	margin: 10px 13px 0 15px;
	font-size: 13px;
	line-height: 15px;
	margin-bottom: 15px;
}
.menubar {
	position: relative;
	vertical-align: middle;
	margin-left: 15px;
	-webkit-border-radius: 8px;
	        border-radius: 8px;
}
.touxiang {
	-webkit-border-radius: 30px;
	        border-radius: 30px;
	width: 20px;
	height: 20px;
	margin-right: 5px;
	vertical-align: middle;
}
.name {
	display: inline-block;
	font-size: 10px;
	margin-left: 7px;
	color: #888888;
}
.time {
	display: inline-block;
	left: 120px;
	font-size: 10px;
	margin-left: 25px;
	color: #888888;
}
.icons {
	position: absolute;
	right: 0;
	width: 103px;
	text-align: right;
	display: inline-block;
	content-flex: right;
	overflow: hidden;
	-webkit-border-bottom-right-radius: 8px;
	        border-bottom-right-radius: 8px;
-webkit-box-shadow:-4px  -2px 2px 0px #888888 inset;
        box-shadow:-4px  -2px 2px 0px #888888 inset;
}
.iconbg{
	background: #FDD041;
	position: absolute;
	width:100%;
	height:50px;
	-webkit-border-top-left-radius: 50px;
	        border-top-left-radius: 50px;
	display: none;
}
.icons image {
	width: 11px;
	height: 11px;
	padding-right: 10px;
}
.picturearea {
	margin: auto;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
}
image {
	width: 30%;
	height: 200rpx;
	margin: auto;
}


.topbar {
	height: 100px;
	background-color: RGB(253, 217, 108);
}
.backk {
	width: 15px;
	height: 10px;
}
.detailtitle {
	width:85%;
	color: white;
	font-size: 14px;
	margin:auto;
	font-weight: 400;
}
.drtailmain {
	-webkit-border-radius: 20px;
	        border-radius: 20px;
	margin-top: -20px;
	background: white;
	-webkit-box-shadow: 0 -1px 8px grey;
	        box-shadow: 0 -1px 8px grey;
}
.detailcontent {
	font-size: 15px;
	width: 85%;
	margin: 20px auto;
}
.detailpics {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
	margin-bottom:30px;
}
.detailpic {
	width: 180rpx;
	height: 180rpx;
	margin: 6px;
}
.articlecard {
	margin: 2px auto 0;
	width: 90%;
	-webkit-border-radius: 5px;
	        border-radius: 5px;
}
.touxiang {
	border-border-radius: 30px;
	width: 20px;
	height: 20px;
	margin-right: 5px;
	vertical-align: middle;
}

